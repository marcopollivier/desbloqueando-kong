{
  "id": null,
  "uid": "kong-gateway-fixed",
  "title": "Kong Gateway Metrics",
  "tags": ["kong", "api-gateway"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 39,
  "version": 1,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Requests",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(increase(kong_http_requests_total[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Request Rate (req/sec)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(kong_http_requests_total[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "unit": "reqps",
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Average Response Time",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(kong_request_latency_ms_sum[5m])) / sum(rate(kong_request_latency_ms_count[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "unit": "ms",
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 500
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Error Rate (%)",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(kong_http_requests_total{code=~\"5..\"}[5m])) / sum(rate(kong_http_requests_total[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "unit": "percent",
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "Request Rate Over Time",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(kong_http_requests_total[1m]))",
          "refId": "A",
          "legendFormat": "Requests/sec",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Response Time Percentiles",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(kong_request_latency_ms_bucket[5m])) by (le))",
          "refId": "A",
          "legendFormat": "50th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(kong_request_latency_ms_bucket[5m])) by (le))",
          "refId": "B",
          "legendFormat": "95th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(kong_request_latency_ms_bucket[5m])) by (le))",
          "refId": "C",
          "legendFormat": "99th percentile",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ms"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 7,
      "title": "HTTP Status Codes",
      "type": "piechart",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(increase(kong_http_requests_total[5m])) by (code)",
          "refId": "A",
          "legendFormat": "{{code}}",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Kong Status",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "kong_datastore_reachable",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "text": "Unreachable"
                },
                "1": {
                  "text": "Healthy"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    }
  ]
}
